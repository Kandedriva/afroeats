# OrderDabaly Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# Backend API URL
REACT_APP_API_BASE_URL=http://localhost:5001

# Application Settings
REACT_APP_APP_NAME="OrderDabaly"
REACT_APP_VERSION=1.0.0

# Environment
NODE_ENV=development

# Stripe Configuration (Frontend)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Development Settings
GENERATE_SOURCEMAP=true
SKIP_PREFLIGHT_CHECK=true

# Production Settings (when NODE_ENV=production)
# REACT_APP_API_BASE_URL=https://api.orderdabaly.com
# REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_your_production_stripe_key
# GENERATE_SOURCEMAP=false