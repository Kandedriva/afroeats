{
  "name": "afro-eats",
  "version": "1.0.0",
  "description": "Afro Eats - Restaurant Ordering Platform",
  "license": "MIT",
  "author": "Afro Eats Team",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0"
  },
  "scripts": {
    "start": "node backend/server.js",
    "dev": "npm run dev:backend",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd afro-eats && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../afro-eats && npm install",
    "build": "cd afro-eats && npm run build"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.8.1"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.68"
  }
}
